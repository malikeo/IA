package misionero;

import java.util.ArrayList;

/**
 *
 * @author Segurity
 */
public class Estado {
    private String movimiento;
    private ArrayList<Nodo> izq,der,bote;
    private Estado antecesor;
    
    public Estado(ArrayList<Nodo> izq ,ArrayList<Nodo> der ,ArrayList<Nodo> bote, Estado antecesor, String movimiento){
        this.izq = izq;
        this.der = der;
        this.bote = bote;
        this.antecesor = antecesor;
        this.movimiento = movimiento;
    }

    public void setMovimiento(String movimiento) {
        this.movimiento = movimiento;
    }

    public void setIzq(ArrayList<Nodo> izq) {
        this.izq = izq;
    }

    public void setDer(ArrayList<Nodo> der) {
        this.der = der;
    }

    public void setBote(ArrayList<Nodo> bote) {
        this.bote = bote;
    }

    public void setAntecesor(Estado antecesor) {
        this.antecesor = antecesor;
    }

    public ArrayList<Nodo> getIzq() {
        return izq;
    }

    public ArrayList<Nodo> getDer() {
        return der;
    }
    //el patrick se la come
    public ArrayList<Nodo> getBote() {
        return bote;
    }

    public Estado getAntecesor() {
        return antecesor;
    }
    
    public String getMovimiento(){
        return movimiento;
    }
    
    
    
    
    
}

